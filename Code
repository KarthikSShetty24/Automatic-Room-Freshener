pinMode(ledPin, OUTPUT);     // Set LED pin as output
}

void loop()
{
    digitalWrite(ledPin, HIGH);   // LED ON
    delay(200);                   // Wait 200ms
    digitalWrite(ledPin, LOW);    // LED OFF
    delay(300);                   // Wait 300ms
    digitalWrite(ledPin, HIGH);  
    delay(200);
    digitalWrite(ledPin, LOW);   
    delay(4000);
    digitalWrite(ledPin, HIGH);  
    delay(300);
    digitalWrite(ledPin, LOW);   
    delay(6000);
}
